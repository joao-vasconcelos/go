{
  "compilerOptions": {
    // Using "baseUrl" allows you to use absolute paths
    "baseUrl": ".",
    // Using "paths" allows you to configure module path aliases
    "paths": {
      "@/components/*": ["components/*"],
      "@/hooks/*": ["go/hooks/*"],
      "@/services/*": ["services/*"],
      "@/schemas/*": ["schemas/*"],
      "@/options/*": ["options/*"],
      "@/contexts/*": ["contexts/*"]
    }
  }
}
